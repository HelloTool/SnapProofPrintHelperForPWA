{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "snap-proof-print-helper",
  "version": "0.1.0",
  "identifier": "io.gitee.jesse205.snap-proof-print-helper",
  "build": {
    "beforeDevCommand": "pnpm dev:tauri:fortend",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm build:tauri:fortend",
    "frontendDist": "../dist/tauri",
    "removeUnusedCommands": true
  },
  "app": {
    "windows": [
      {
        "title": "snap-proof-print-helper",
        "width": 1200,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}