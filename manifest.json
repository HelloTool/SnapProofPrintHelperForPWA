    },
    {
      "src": "<%= process.env.BASE_URL %>images/icons/maskable_icon_x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "<%= process.env.BASE_URL %>images/icons/maskable_icon_x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "<%= process.env.BASE_URL %>images/icons/maskable_icon.png",
      "sizes": "1024x1024",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "<%= process.env.BASE_URL %>images/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "<%= process.env.BASE_URL %>images/icons/icon-512x512.png",
      "sizes": "512x512",
